{
  "compilerOptions": {
    "lib": ["dom", "dom.iterable", "esnext"],
    "allowJs": true,
    "skipLibCheck": true,
    "strict": true,
    "noEmit": true,
    "esModuleInterop": true,
    "module": "esnext",
    "moduleResolution": "bundler",
    "resolveJsonModule": true,
    "isolatedModules": true,
    "jsx": "preserve",
    "incremental": true,
    "plugins": [
      {
        "name": "next"
      }
    ],
    "paths": {
      "@/*": ["./src/*"]
    }
  },
  "include": ["next-env.d.ts", "**/*.ts", "**/*.tsx", ".next/types/**/*.ts", "src/pages/api/tabela.js", "src/components/organisms/Sections/ParallaxSection/ParallaxSection.jsx", "src/components/organisms/Sections/Sobre/index.jsx", "src/components/atoms/CurvedTextFooter/CurvedTextFooter.jsx", "src/components/molecules/FooterCurved/FooterCurved.jsx", "src/pages/index.jsx", "src/components/organisms/Sections/Valores/Valores.jsx"],
  "exclude": ["node_modules"]
}
